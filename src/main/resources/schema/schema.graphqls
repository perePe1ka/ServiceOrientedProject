type Query {
    getAllProducts: [Product]
    getProduct(productUUID: String!): Product
    deleteProduct(productUUID: String!): Product
}

type Product {
    uuid: String
    name: String
    description: String
    price: Float
}

type Orders {
    uuid: String!
    customerName: String!
    customerAddress: String!
    orderCost: Float!
}

